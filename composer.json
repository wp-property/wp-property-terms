{
  "name": "wp-property/wp-property-terms",
  "type": "wordpress-plugin",
  "minimum-stability": "dev",
  "homepage": "https://usabilitydynamics.com",
  "author": {
    "name": "UsabilityDynamics, Inc.",
    "url": "https://usabilitydynamics.com"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wp-property/wp-property-terms.git"
  },
  "require": {
    "php": ">=5.3",
    "usabilitydynamics/lib-wp-bootstrap": "*"
  },
  "require-dev": {
    "usabilitydynamics/lib-wp-phpunit": "~0.2",
    "codeclimate/php-test-reporter": "~0.1"
  },
  "autoload": {
    "classmap": [ "lib/classes" ]
  },
  "archive": {
    "exclude": [
      "vendor",
      "node_modules",
      ".gitignore",
      "test",
      "circle.yml",
      "composer.lock",
      "gruntfile.js",
      "package.json"
    ]
  },
  "extra": {
    "schemas": {
      "dependencies" : {
        "modules": {},
        "plugins": []
      },
      "licenses" : {
        "client" : false,
        "product" : false
      }
    }
  }
}